{
  "name": "rstful",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "bun --filter @rstful/web dev",
    "dev:api": "bun --filter @rstful/api dev",
    "build": "bun --filter @rstful/web build",
    "start": "bun --filter @rstful/web start",
    "typecheck": "bun --filter @rstful/web typecheck"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}